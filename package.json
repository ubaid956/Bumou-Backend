  "scripts": {
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:rum": "docker-compose rm -s -f -v dev-db",
    "db:dev:up": "docker compose up dev-db -d",
    "db:dev:restart": "yarn db:dev:rum && yarn db:dev:up && sleep 2 && yarn prisma:dev:deploy",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pm2:deploy:app": "npm run build && pm2 start app.json",
    "pm2:start:app": "pm2 start app.json",
    "pm2:stop:app": "pm2 stop app.json",
    "pm2:destroy:app": "pm2 delete app.json",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@alicloud/endpoint-util": "^0.0.1",
